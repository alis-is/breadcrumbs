{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 16",
  
    "compilerOptions": {
      "lib": ["es2021"],
      "module": "commonjs",
      "target": "es2021",
      "baseUrl": ".",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true, 
      "noImplicitAny": false
    }
  }